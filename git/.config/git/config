# Local/private config goes in the include
[include]
    path = conf.d/aliases
    path = conf.d/colors
    path = conf.d/private

[fetch]
    prune = true

[merge]
    conflictstyle = diff3

[pager]
    status = true

[push]
    default = current

[pull]
    rebase = true

[rebase]
	autoStash = true

[init]
	defaultBranch = main
