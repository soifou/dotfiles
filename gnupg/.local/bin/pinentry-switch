#!/usr/bin/env sh

case $PINENTRY_USER_DATA in
    tty) exec /usr/bin/pinentry-curses "$@" ;;
    rofi) exec "$HOME"/.local/bin/rofi/pinentry-rofi "$@" ;;
    none)
        echo "pinentry-switcher Error"
        exit 1 # do not ask for passphrase
        ;;
    *) exec /usr/bin/pinentry "$@" ;;
esac
